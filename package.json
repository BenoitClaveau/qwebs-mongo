{
    "name": "qwebs-mongo",
    "version": "0.1.19",
    "description": "Mongo client for your Qwebs server",
    "main": "index.js",
    "dependencies": {
        "mongodb": "latest",
        "qwebs": "^0.1.19"
    },
    "devDependencies": {
        "jasmine-node": "latest",
        "coveralls": "latest",
        "istanbul": "latest"
    },
    "scripts": {
        "pretest": "start mongod.exe --dbpath tests/data/db",
        "test": "node node_modules/jasmine-node/bin/jasmine-node tests --verbose",
        "test-cover": "istanbul cover node_modules/jasmine-node/bin/jasmine-node --captureExceptions tests/*.spec.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/beny78/qwebs-mongo.git"
    },
    "keywords": [
        "qwebs",
        "mongo",
        "mongodb",
        "client",
        "web",
        "database",
        "promise"
    ],
    "author": {
        "name": "Beno√Æt Claveau",
        "email": "beny78@gmail.com"
    },
